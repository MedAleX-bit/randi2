<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <local-check-config name="RANDI2" location="conf/debug/randi2_checks.xml" type="project" description="">
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="RANDI2 Checks" enabled="true" check-config-name="RANDI2" local="true">
        <file-match-pattern match-pattern=".java$" include-pattern="true"/>
        <file-match-pattern match-pattern=".*test.*" include-pattern="false"/>
        <file-match-pattern match-pattern=".*Dummy.*" include-pattern="false"/>
    </fileset>
    <filter name="FilesFromPackage" enabled="true">
        <filter-data value="src/de/randi2/junittests"/>
        <filter-data value="src/de/randi2/seleniumtest"/>
    </filter>
</fileset-config>
